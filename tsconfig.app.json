{
  "compilerOptions": {
    "strict": false,
    "allowSyntheticDefaultImports": true,
    "noEmitOnError": false,
    "skipLibCheck": true,
    "allowJs": true,
    "checkJs": false,
    "moduleResolution":"node",
    "isolatedModules": false,
    "target": "ES2020",
    "module": "ESNext",
    "jsx": "react-jsx",
    "lib": ["ES2020", "DOM", "DOM.Iterable"]
  },
  "include": ["src"]
}
